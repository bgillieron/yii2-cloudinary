{
    "name": "bgillieron/yii2-cloudinary",
    "description": "Cloudinary integration for Yii2 framework",
    "type": "yii2-extension",
    "keywords": ["yii2", "cloudinary", "image", "upload"],
    "license": "MIT",
    "authors": [
      {
        "name": "Brendon Gilli√©ron",
        "email": "b.huka6@passmail.net"
      }
    ],
    "require": {
      "yiisoft/yii2": "~2.0.0",
      "cloudinary/cloudinary_php": "^2.0"
    },
    "autoload": {
      "psr-4": {
        "yii2cloudinary\\": "src/"
      }
    },
    "extra": {
      "yii\\composer\\Installer::postInstall": [
        "yii\\composer\\Callback::addYiiPath"
      ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
  }
  